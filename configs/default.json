{
  "verbosity": "info",
  "address": "0.0.0.0:2053",
  "table": {
    "start": {
      "if": "any",
      "then": [
        {
          "query": "cloudflare"
        },
        "end"
      ],
    }
  },
  "upstreams": {
    "cloudflare": {
      "https": {
        "timeout": 4,
        "addr": "https://1.0.0.1:443/dns-query"
      }
    }
  }
}
